<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.short_term.crm.mapper.EmpMapper">
    <select id="login" resultType="com.short_term.crm.entity.Emp">
        select * from emp where
        logid = #{param.logid}
        and logpwd = #{param.logpwd}
    </select>
</mapper>
